{
  "name": "delineate-hackathon-challenge-cuet-fest-25-nodejs",
  "private": true,
  "type": "module",
  "packageManager": "npm@10.9.0",
  "engines": {
    "node": ">=24.10.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "format": "turbo format",
    "format:check": "turbo format:check",
    "check-types": "turbo check-types",
    "docker:dev": "docker compose -f docker/compose.dev.yml up --build",
    "docker:prod": "docker compose -f docker/compose.prod.yml up --build -d"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "turbo": "^2.3.3",
    "typescript-eslint": "^8.46.4"
  }
}
