{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 80,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "arrowParens": "always",
  "endOfLine": "lf",
  "tabWidth": 2,
  "useTabs": false,
  "overrides": [{ "files": "*.md", "options": { "proseWrap": "always" } }]
}
